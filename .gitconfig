[credential]
	helper = osxkeychain
[core]
	editor = vim
[alias]
	co = checkout
	ci = commit
	br = branch
[fetch]
	prune = true

[includeIf "gitdir:~/project/gogo-boot/"]
	path = ~/project/gogo-boot/my-setting/.gitconfig-personal

[includeIf "gitdir:~/project/ifp/**"]
	path = ~/project/ifp/.gitconfig-work
